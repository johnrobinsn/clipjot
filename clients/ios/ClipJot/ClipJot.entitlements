<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <!-- App Groups for sharing data with Share Extension -->
    <key>com.apple.security.application-groups</key>
    <array>
        <string>group.net.clipjot.shared</string>
    </array>

    <!-- Keychain Access Groups for sharing tokens with Share Extension -->
    <key>keychain-access-groups</key>
    <array>
        <string>$(AppIdentifierPrefix)net.clipjot.shared</string>
    </array>
</dict>
</plist>
